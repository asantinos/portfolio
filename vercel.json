{
    "rewrites": [
        {
            "source": "/:path*",
            "has": [
                {
                    "type": "host",
                    "value": "stride.asantinos.com"
                }
            ],
            "destination": "/apps/stride/:path*"
        },
        {
            "source": "/:path*",
            "has": [
                {
                    "type": "host",
                    "value": "monked.asantinos.com"
                }
            ],
            "destination": "/apps/monked/:path*"
        }
    ]
}