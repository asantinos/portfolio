{
    "redirects": [
        {
            "source": "/apps/stride/:path*",
            "destination": "https://stride.asantinos.com/:path*",
            "permanent": true
        },
        {
            "source": "/apps/monked/:path*",
            "destination": "https://monked.asantinos.com/:path*",
            "permanent": true
        }
    ],
    "rewrites": [
        {
            "source": "/:path*",
            "has": [
                {
                    "type": "host",
                    "value": "stride.asantinos.com"
                }
            ],
            "destination": "/apps/stride/:path*"
        },
        {
            "source": "/:path*",
            "has": [
                {
                    "type": "host",
                    "value": "monked.asantinos.com"
                }
            ],
            "destination": "/apps/monked/:path*"
        }
    ]
}